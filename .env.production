# =================================================================
# üö® CRITICAL PRODUCTION SETTINGS
# =================================================================

# Production Mode (REQUIRED)
DISABLE_DEMO_MODE=true
ENVIRONMENT=production

# Application Configuration
SECRET_KEY=your-secure-secret-key-generate-new-one
HOST=0.0.0.0
PORT=5000
LOG_LEVEL=INFO

# =================================================================
# üî• FIREBASE CONFIGURATION (REQUIRED FOR PRODUCTION)
# =================================================================

# Firebase Project Settings
FIREBASE_PROJECT_ID=your-real-project-id
FIREBASE_APP_ID=your-real-app-id
FIREBASE_API_KEY=your-real-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id

# Firebase Service Account (REQUIRED)
GOOGLE_APPLICATION_CREDENTIALS=./your_production_service_account.json

# =================================================================
# üìä SPORTS DATA APIs (REQUIRED FOR LIVE DATA)
# =================================================================

# Primary Sports API (The Odds API recommended)
SPORTS_API_KEY=your-sports-api-key

# Optional: Additional Sports APIs
ODDS_API_KEY=your-odds-api-key
WEATHER_API_KEY=your-weather-api-key

# =================================================================
# üí∞ SPORTSBOOK APIs (OPTIONAL - FOR LIVE BETTING)
# =================================================================

# Sportsbook Integration (Optional)
DRAFTKINGS_API_KEY=your-draftkings-api-key
FANDUEL_API_KEY=your-fanduel-api-key
BETMGM_API_KEY=your-betmgm-api-key

# Betting Configuration
ENABLE_REAL_BETTING=false
RATE_LIMIT_PER_HOUR=500
API_TIMEOUT_SECONDS=30

# =================================================================
# ü§ñ MACHINE LEARNING CONFIGURATION
# =================================================================

# ML Model Configuration
MODEL_STORAGE_PATH=./models
MAX_TRAINING_SAMPLES=10000
DEFAULT_CONFIDENCE_THRESHOLD=0.65
ENABLE_MODEL_CACHING=true

# =================================================================
# üöÄ DEPLOYMENT CONFIGURATION
# =================================================================

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=false

# Database Configuration (Optional overrides)
# DATABASE_URL=your-database-url-if-needed

